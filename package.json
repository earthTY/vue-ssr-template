    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node server/server.js",
    "clean": "rimraf public && rimraf server-build",
    "build:client": "cross-env NODE_ENV=production webpack --config ./build/webpack.client.config.js",
    "build:server": "cross-env NODE_ENV=production webpack --config ./build/webpack.server.config.js",
    "build": "npm run clean && npm run build:client && npm run build:server",
    "dev:client": "cross-env NODE_ENV=development webpack-dev-server --disableHostCheck=true --config ./build/webpack.client.config.js",
    "dev:server": "cross-env NODE_ENV=development nodemon server/server.js",
    "dev": "concurrently \"npm run dev:client\" \"npm run dev:server\""
  },
